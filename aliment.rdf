        <rdfs:subClassOf rdf:resource="&aliment;Anis"/>
        <owl:disjointWith rdf:resource="&aliment;AnisVert"/>
        <skos:altLabel xml:lang="en">blue giant hysop</skos:altLabel>
        <skos:altLabel xml:lang="en">fragrant giant hyssop</skos:altLabel>
        <rdfs:comment xml:lang="en">http://en.wikipedia.org/wiki/Agastache_foeniculum</rdfs:comment>
        <skos:altLabel xml:lang="en">lavendar giant hyssop</skos:altLabel>
        <rdfs:comment xml:lang="fr">Elle est d&apos;abord consommée en infusion ou en thé et devient par la suite une plante aromatique et condimentaire qui parfume crudités, salades, boissons, gâteaux.</rdfs:comment>
        <skos:altLabel xml:lang="fr">agastache</skos:altLabel>
        <rdfs:isDefinedBy rdf:resource="http://openfoodsystem.org/ontology/aliment"/>
    </owl:Class>
    


    <!-- http://openfoodsystem.org/ontology/aliment#AnisVert -->

    <owl:Class rdf:about="&aliment;AnisVert">
        <rdfs:label xml:lang="en">green anise</rdfs:label>
        <rdfs:label xml:lang="fr">anis vert</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aliment;Anis"/>
        <ofs:aPourCodeLangual>B1296</ofs:aPourCodeLangual>
        <skos:altLabel xml:lang="en">Pimpinella anisum</skos:altLabel>
        <rdfs:comment xml:lang="en">Same as anise in EN. Compare: http://en.wikipedia.org/wiki/Anise
http://fr.wikipedia.org/wiki/Anis_vert</rdfs:comment>
        <skos:altLabel xml:lang="en">anise</skos:altLabel>
        <rdfs:comment xml:lang="fr">Les feuilles fraîches finement hachées servent à aromatiser certains plats : crudités, salades, potages, etc.
Les graines sont utilisées en pâtisserie et en confiserie (Anis de Flavigny, pain d&apos;épices, dragées, etc.).
Elles entrent également dans la composition de liqueurs et boissons anisées (anisette, rakı, ouzo, pastis, arak, pontarlier, absinthe, etc.).</rdfs:comment>
        <skos:altLabel xml:lang="fr">anis boucage</skos:altLabel>
        <skos:altLabel xml:lang="fr">boucage à fruits suaves</skos:altLabel>
        <skos:altLabel xml:lang="fr">pimpinelle anis</skos:altLabel>
        <rdfs:isDefinedBy rdf:resource="http://openfoodsystem.org/ontology/aliment"/>
    </owl:Class>
    


