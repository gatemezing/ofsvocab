        <rdfs:label xml:lang="fr">poire de boeuf</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aliment;TendeDeTranche"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&aliment;aPourMoisPleineSaison"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&aliment;Aout"/>
                            <rdf:Description rdf:about="&aliment;Avril"/>
                            <rdf:Description rdf:about="&aliment;Decembre"/>
                            <rdf:Description rdf:about="&aliment;Fevrier"/>
