        <rdfs:label xml:lang="fr">chou vert</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aliment;Chou"/>
        <skos:altLabel xml:lang="en">collards</skos:altLabel>
        <rdfs:comment xml:lang="en">http://en.wikipedia.org/wiki/Collard_greens</rdfs:comment>
