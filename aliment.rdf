    </owl:Class>
    


    <!-- http://openfoodsystem.org/ontology/aliment#Chouquette -->

    <owl:Class rdf:about="&aliment;Chouquette">
        <rdfs:label xml:lang="en">chouquette</rdfs:label>
        <rdfs:label xml:lang="fr">chouquette</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aliment;ProduitDeViennoiserie"/>
