    </owl:Class>
    


    <!-- http://openfoodsystem.org/ontology/aliment#Annabelle -->

    <owl:Class rdf:about="&aliment;Annabelle">
        <rdfs:label xml:lang="en">Annabelle potato</rdfs:label>
