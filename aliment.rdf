        <rdfs:label xml:lang="fr">fromage de chèvre</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&aliment;Fromage"/>
