    <!-- http://openfoodsystem.org/ontology/aliment#DeliceDeChartreuse -->

    <owl:Class rdf:about="&aliment;DeliceDeChartreuse">
        <rdfs:label xml:lang="en">Délice de Chartreuse cheese</rdfs:label>
        <rdfs:label xml:lang="fr">délice de Chartreuse</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aliment;PMCrouteLavee"/>
