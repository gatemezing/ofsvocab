        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&aliment;aPourRemplacant"/>
                <owl:someValuesFrom rdf:resource="&aliment;Echalote"/>
