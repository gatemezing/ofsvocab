    <!-- http://openfoodsystem.org/ontology/aliment#PommeDeTerreAChairFerme -->

    <owl:Class rdf:about="&aliment;PommeDeTerreAChairFerme">
        <rdfs:label xml:lang="en">firm potato</rdfs:label>
        <rdfs:label xml:lang="fr">pomme de terre à chair ferme</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
