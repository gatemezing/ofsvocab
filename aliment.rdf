        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&aliment;aPourTauxMatiereGrasse"/>
                <owl:hasValue rdf:datatype="&xsd;integer">45</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
