    </owl:Class>
    


    <!-- http://openfoodsystem.org/ontology/aliment#Pleurote -->

    <owl:Class rdf:about="&aliment;Pleurote">
        <rdfs:label xml:lang="en">oyster mushroom</rdfs:label>
        <rdfs:label xml:lang="fr">pleurote</rdfs:label>
