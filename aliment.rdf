    <!-- http://openfoodsystem.org/ontology/aliment#Riz -->

    <owl:Class rdf:about="&aliment;Riz">
        <rdfs:label xml:lang="en">rice</rdfs:label>
        <rdfs:label xml:lang="fr">riz</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aliment;Cereale"/>
