    <!-- http://openfoodsystem.org/ontology/aliment#Gomasio -->

    <owl:Class rdf:about="&aliment;Gomasio">
        <rdfs:label xml:lang="en">gomasio</rdfs:label>
        <rdfs:label xml:lang="fr">gomasio</rdfs:label>
